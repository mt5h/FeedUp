<li><a href="/feed/dashboard "><%= __('your_dashboard') %><i class="material-icons indigo-text">dashboard</i></a></li>
<li><a class='dropdown-trigger' href='#' data-target='favorite'><%= __('your_favorites') %><i class="material-icons indigo-text">favorite</i></a></li>
	<ul id='favorite' class='dropdown-content'>
		<% feeds.forEach(function(el){ %>
			<% if (el.favorite === true) { %>
			<li>
				<div class="row dropdown-row">
					<div class="col s9 center-align">
						<a href="/feed/dashboard/<%= el.id %>" name="<%= el.id %>" class="query truncate indigo-text"> <%= el.title %> </a>
					</div>
					<div class="col s3 right align-center">
						<a href="#" name="<%= el.id %>" class="btn-floating transparent z-depth-0" onclick="update_modal(this)"><i class="tiny indigo-text material-icons">edit</i></a>
					</div>
				</div>
			</li>
			<% } %>
		<% }) %>
	</ul>
<li><a class='dropdown-trigger' href='#' data-target='education'><%= __('education') %><i class="material-icons indigo-text">school</i></a></li>
	<ul id='education' class='dropdown-content'>
		<% var item_counter = 0; var category = "education";  %>
		<%- partial('../layouts/partial-feed-dropdown.ejs',{'feeds': feeds,'category':category, "item_counter": item_counter}) %>
	</ul>
